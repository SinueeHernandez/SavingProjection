<div class="container">
<h2>Menu Details</h2>
<form class="form-horizontal">
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @menu.name %>
</p>

<p>
  <strong>Level:</strong>
  <%= @menu.level %>
</p>

<p>
  <strong>Parent:</strong>
  <%= @menu.parent %>
</p>

<p>
  <strong>Icon:</strong>
  <%= @menu.icon %>
</p>

<p>
  <strong>Path:</strong>
  <%= @menu.path %>
</p>
</form>
<%= link_to '<button type="button" class="btn btn-primary">Edit</button>'.html_safe, edit_menu_path(@menu) %> |
<%= link_to '<button type="button" class="btn btn-primary">Back</button>'.html_safe, menus_path %>
</div>